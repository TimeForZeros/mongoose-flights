<table id="list">
  <thead>
    <tr>
      <th>Airline</th>
      <th>Airport</th>
      <th>Flight Number</th>
      <th>Departure</th>

    </tr>
  </thead>
  <tbody>
    <% flights.forEach(function(f) { %>
    <tr>
      <td><%= f.airline %></td>
      <td><%= f.airport %></td>
      <td><%= f.flightNo %></td>
      <td><%= f.departs %></td>
      <td><a href="/flights/<%= f._id %>">DETAILS</a></td>
    </tr>
    <% }); %>
  </tbody>
</table>
<p><a href="/flights/new">Add a New Flight</a></p>